# Database configuration for PostgreSQL
POSTGRES_HOST=                  # Host where the PostgreSQL database is running (e.g., localhost)
POSTGRES_PORT=                  # Port on which PostgreSQL is listening (e.g., 5432)
POSTGRES_USER=                  # Username to connect to PostgreSQL (e.g., postgres)
POSTGRES_PASSWORD=              # Password for the PostgreSQL user (e.g., your_password)
POSTGRES_DATABASE=              # Name of the PostgreSQL database to connect to (e.g., core_test)

# HMAC key for signing or verifying messages
HMAC=                           # HMAC key used for cryptographic operations (e.g., your_hmac_key)

# KFTC (Korea Financial Telecommunications & Clearings Institute) settings
KFTC_ORG_CODE=                  # Organization code for KFTC integration (e.g., T2103A0015)

# Base URLs for external services
COREA_BASE_URL=                 # Base URL for the CoreA service (e.g., http://localhost:7990)
OLIVE_BASE_URL=                 # Base URL for the Olive service (e.g., http://localhost:7991)

# Environment configuration
NODE_ENV=                       # Node.js environment (e.g., development, test, production)

# Encryption key for internal use
INTERNAL_ENCRYPT_KEY=           # Key used for internal encryption operations (e.g., your_internal_encrypt_key)
